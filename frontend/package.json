{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.6.2",
    "@mui/styles": "^5.15.0",
    "@reduxjs/toolkit": "^1.5.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.1",
    "react-scripts": "5.0.1",
    "redux": "^4.0.5",
    "typescript": "^5.3.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "PORT=3001 react-scripts start",
    "windows:dev": "set PORT=3001 react-scripts start",
    "cheap:build": "GENERATE_SOURCEMAP=false react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "http-server": "http-server -b -g -p 3001 --proxy http://localhost:3001? build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/no-anonymous-default-export": 0
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      "safari>=10",
      "Chrome>=23",
      "ff>=28",
      "Edge>=79",
      "and_chr>=23",
      "and_ff>=28",
      "Samsung>=4",
      "iOS>=10",
      "not dead"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.4.1",
    "@types/moment-jalaali": "^0.7.8",
    "@types/node": "^17.0.26",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "@types/stylis": "^4.2.1",
    "@types/toastify-js": "^1.12.1",
    "prettier": "^3.0.3"
  }
}
